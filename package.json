{
  "name": "council-cli",
  "version": "1.0.0",
  "description": "CLI совет из нескольких LLM с председателем-агентом",
  "type": "module",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StanislavHerman/House-of-Collective-Intelligence.git"
  },
  "keywords": [
    "ai",
    "cli",
    "llm",
    "agent",
    "council",
    "openai",
    "anthropic",
    "terminal"
  ],
  "author": "Stanislav Herman",
  "license": "MIT",
  "bin": {
    "council": "dist/index.js",
    "hause": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "postinstall": "npm run build",
    "setup": "node -e \"if (process.platform === 'win32') { require('child_process').execSync('powershell -ExecutionPolicy Bypass -File install.ps1', { stdio: 'inherit' }); } else { require('child_process').execSync('chmod +x install.sh && ./install.sh', { stdio: 'inherit' }); }\""
  },
  "dependencies": {
    "axios": "^1.7.9",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "form-data": "^4.0.5",
    "ora": "^8.1.0",
    "puppeteer": "^24.33.0",
    "xcode": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "typescript": "^5.6.3"
  }
}
